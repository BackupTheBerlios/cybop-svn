<?xml version="1.0" encoding="ISO-8859-1"?>

<model>
    <part name="menu_bar" channel="file" abstraction="compound" model="ui_control/gui/main_window/menu_bar.cybol">
        <property name="layout" channel="inline" abstraction="character" model="compass"/>
        <property name="cell" channel="inline" abstraction="character" model="north"/>
        <property name="position" channel="inline" abstraction="integer" model="0,0,0"/>
        <property name="size" channel="inline" abstraction="integer" model="780,20,1"/>
        <property name="left_release" channel="inline" abstraction="knowledge" model=".ui_control.logic.exit_program"/>
    </part>
