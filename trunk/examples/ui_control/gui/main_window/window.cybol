    <part name="status_bar" channel="file" abstraction="compound" model="ui_control/gui/main_window/status_bar.cybol">
        <property name="layout" channel="inline" abstraction="character" model="compass"/>
        <property name="cell" channel="inline" abstraction="character" model="south"/>
        <property name="position" channel="inline" abstraction="integer" model="0,20,0"/>
        <property name="size" channel="inline" abstraction="integer" model="780,20,1"/>
    </part>
