<model>
    <part name="model" channel="file" abstraction="cybol" model="part.cybol">
        <property name="minimum" channel="inline" abstraction="integer" model="0"/>
        <property name="maximum" channel="inline" abstraction="integer" model="unbounded"/>
    </part>
</model>

<model>
