        <property name="maximum" channel="inline" abstraction="integer" model="unbounded"/>
        <property name="name" channel="inline" abstraction="character" model=""/>
        <property name="channel" channel="inline" abstraction="character" model="">
            <constraint name="choice" channel="inline" abstraction="character" model="inline,file,http,ftp"/>
        </property>
        <property name="abstraction" channel="inline" abstraction="character" model="">
            <constraint name="choice" channel="inline" abstraction="character" model="cybol,knowledge,boolean,integer,float,character"/>
        </property>
        <property name="model" channel="inline" abstraction="character" model=""/>
    </part>
</model>

